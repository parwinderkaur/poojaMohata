<!-- Blog Detail Section Start -->
<div class="ast_blog_wrapper ast_toppadder80 ast_bottompadder80">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 col-md-12 col-sm-12 col-12">
        <div class="ast_blog_box">
          <div class="ast_blog_img">
            <span class="ast_date_tag"><%= moment(blog.dateOfPost).format('DD MMMM, YYYY') %></span>
            <img src="<%= blog.image %>" alt="<%= blog.title %>" title="<%= blog.title %>">
          </div>
          <div class="ast_blog_info">
            <h3 class="ast_blog_info_heading"><%= blog.title %></h3>
            <p class="ast_blog_info_details"><%= blog.description %></p>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<!--Blog section end-->
