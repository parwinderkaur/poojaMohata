<!-- Header Start -->
<div class="ast_top_header">
	<div class="container">
		<div class="row">
			<div class="col-lg-12 col-md-12 col-sm-12 col-12">
				<div class="ast_contact_details">
					<ul>
                        <% if (settings?.number) { %>
							<li><i class="fa fa-phone" aria-hidden="true"></i><%= settings.number %></li>
                        <% } %>
                        <% if (settings?.email) { %>
						<li><a href="#"><i class="fa fa-envelope-o" aria-hidden="true"></i>
                             <span class="__cf_email__" data-cfemail="14676164647b666054637176677d60713a777b79"><%= settings.email %></span></a></li>
                             <% } %>
					</ul>
				</div>
				<div class="ast_autho_wrapper">
					<ul>
                        <% if (settings?.facebookLink) { %>
							<li><a href="<%= settings.facebookLink %>" target="_blank"><i class="fa fa-facebook-square bg-facebook-o" aria-hidden="true"></i></a></li>
						<% } %>
						<% if (settings?.instagramLink) { %>
							<li><a href="<%= settings.instagramLink %>" target="_blank"><i class="fa fa-instagram bg-instagram-o" aria-hidden="true"></i></a></li>
						<% } %>
						<% if (settings?.whatsappLink) { %>
							<li><a href="<%= settings.whatsappLink %>" target="_blank"><i class="fa fa-whatsapp bg-whatsapp-o" aria-hidden="true"></i></a></li>
						<% } %>
                    </ul><!---->
				</div>
			</div>
		</div>
	</div>
</div>
<div class="ast_header_bottom">
	<div class="container">
		<div class="row">
			<div class="col-lg-3 col-md-3 col-sm-3 col-10">
				<div class="ast_logo">
					<a href="index"><img src="<%= settings?.logoImage || '/images/header/logo.png' %>" width="100" alt="Logo" title="Logo"></a>
				</div>
			</div>
			<div class="col-lg-9 col-md-9 col-sm-9 col-2">
				<div class="ast_main_menu_wrapper">
					<div class="ast_menu">
						<ul>
							<li><a href="/" class="<%= currentPath === '/' ? 'active' : '' %>">Home</a></li>
							<li><a href="/services" class="<%= currentPath.startsWith('/services') ? 'active' : '' %>">Services</a></li>
							<li><a href="/blogs" class="<%= currentPath.startsWith('/blogs') ? 'active' : '' %>">Blog</a></li>
							<li><a href="/shop" class="<%= currentPath.startsWith('/shop') ? 'active' : '' %>">Product</a></li>
							<li><a href="/contact" class="<%= currentPath === '/contact' ? 'active' : '' %>">Contact</a></li>
							<li><a href="/about" class="<%= currentPath === '/about' ? 'active' : '' %>">About</a></li>
							<li><a href="/privacy_policy" class="<%= currentPath === '/privacy_policy' ? 'active' : '' %>">Privacy Policy</a></li>
							<li><a href="/faq" class="<%= currentPath === '/faq' ? 'active' : '' %>">FAQ</a></li>
						</ul>
					</div>
				</div>
				<button class="ast_menu_btn"><i class="fa fa-bars" aria-hidden="true"></i></button>
			</div>
		</div>
	</div>
</div>
<!-- Header End -->    